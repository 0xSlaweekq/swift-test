  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "npx nx reset && npx nx run-many -t=build --parallel=2",
    "format": "npx prettier -w \"./**/*{.ts,.js,.json,.md}\"",
    "lint": "npx eslint --fix \"./**/*{.ts,.js,.tsx,.jsx}\"",
    "lint:nx": "npm run nx -- run-many --t lint",
    "lint:nx-fix": "nx run-many --t lint --fix && nx format:write --all",
    "ln": "docker compose -f ./docker-compose.yml up -d",
    "ln-reboot": "docker compose -f ./docker-compose.yml down -v && docker compose -f ./docker-compose.yml rm -v && npm run ln",
    "ln-stop": "docker compose -f ./docker-compose.yml down -v && docker compose -f ./docker-compose.yml rm -v",
    "migrate:down": "npm run typeorm migration:revert -- -d ./src/configs/orm-migration.config.ts",
    "migrate:up": "npm run typeorm migration:run -- -d ./src/configs/orm-migration.config.ts",
    "migration:create": "npm run typeorm migration:create",
    "migration:generate": "npm run typeorm migration:generate -- -d ./apps/server/src/configs/orm-migration.config.ts",
    "nx": "nx",
    "pm2": "pm2",
    "pm2:r": "npm run pm2 reload ecosystem.config.js",
    "pm2:s": "npm run pm2 save",
    "server:build": "npx nx reset && npx nx run server:build",
    "server:start": "npx nx reset && npx nx run server:serve",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typeorm": "npx ts-node -r tsconfig-paths/register --project ./tsconfig.json ./node_modules/typeorm/cli.js",
    "web:build": "npx nx reset && npx nx run web:build",
    "web:start": "npx nx reset && npx nx run web:serve:dev"
  },
